    <div class="intro-header cWhite">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="intro-message">
                        <h1>Weiss Wedding</h1>
                        <h3>May 20, 2017</h3>
                        <hr class="intro-divider">
                    </div>
                </div>
            </div>

        </div>

    </div>
     <div class="modal fade" id="login-modal" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
      <div class="modal-dialog">
        <div class="loginmodal-container">
          <%= form_for(resource, 
                :as => resource_name,
                :url => session_path(resource_name),
                :html => {:id => "sign_in_user"},
                :format => :json) do |f| %>

            <div class="field">
              <p><%= f.label :login %><br />
              <%= f.text_field :login %></p>
            </div>

            <div class="field">
              <%= f.label :password %><br />
              <%= f.password_field :password, autocomplete: "off" %>
            </div>

            <% if devise_mapping.rememberable? -%>
              <div class="field">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me %>
              </div>
            <% end -%>

            <div class="actions">
              <input class="login loginmodal-submit" type="submit" name="commit" value="Log in" data-disable-with="Log in" />
            </div>
          <% end %>
        </div>
      </div>
    </div>
    <%= javascript_include_tag "landing", "data-turbolinks-track" => true  %>

